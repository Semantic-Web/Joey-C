(DEFINE-PRIMITIVE-ROLE |hasAge|) 
(DEFINE-PRIMITIVE-ROLE |hasPet|) 
(IMPLIES MAN PERSON) 
(IMPLIES WOMAN PERSON) 
(IMPLIES WOMAN (NOT MAN)) 
(IMPLIES MAN (NOT WOMAN)) 
(IMPLIES DOG ANIMAL) 
(IMPLIES CAT ANIMAL) 
(IMPLIES DOG (NOT CAT)) 
(IMPLIES OLD (NOT YOUNG)) 
(IMPLIES YOUNG AGE) 
(IMPLIES OLD AGE) 
(DEFINE-CONCEPT OLDLADY (AND (SOME |hasPet| TOP) (ALL |hasPet| CAT) (ALL |hasAge| OLD) WOMAN)) 
(DEFINE-CONCEPT DOGOWNER (AND (SOME |hasPet| DOG) PERSON)) 
(DEFINE-CONCEPT CATOWNER (AND (SOME |hasPet| CAT) PERSON)) 
(DEFINE-CONCEPT PETOWNER (AND (SOME |hasPet| ANIMAL) PERSON)) 
(DEFINE-CONCEPT CATHATER (AND (ALL |hasPet| (NOT CAT)) PERSON)) 
(DEFINE-CONCEPT DOGHATER (AND (ALL |hasPet| (NOT DOG)) PERSON)) 
(DEFINE-CONCEPT CATLIKER (NOT CATHATER)) 
