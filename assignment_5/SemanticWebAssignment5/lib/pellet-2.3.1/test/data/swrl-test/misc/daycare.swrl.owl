                            <has_teacher rdf:resource="#miss_Lynn"/>
                            <has_date_of_birth rdf:datatype="http://www.w3.org/2001/XMLSchema#date"
                            >2005-03-14</has_date_of_birth>
                          </Student>
                        </teaches_student>
                        <teaches_student>
                          <Student rdf:ID="ella">
                            <has_teacher rdf:resource="#miss_Lynn"/>
                            <has_age rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
