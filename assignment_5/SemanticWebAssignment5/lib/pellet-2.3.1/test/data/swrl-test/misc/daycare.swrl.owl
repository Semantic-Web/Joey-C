  <swrl:Variable rdf:ID="x"/>
  <swrl:Imp rdf:ID="def-attends_classroom">
    <swrl:body>
      <swrl:AtomList>
        <rdf:first>
          <swrl:ClassAtom>
            <swrl:argument1 rdf:resource="#x"/>
